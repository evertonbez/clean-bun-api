generator client {
  provider = "prisma-client-js"
  output   = "../src/infra/database/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Account {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime @default(now())

  @@map("accounts")
}
